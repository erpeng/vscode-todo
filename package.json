{
    "name": "todo",
    "displayName": "todo",
    "description": "VS Code TODO File Extension",
    "version": "0.1.0",
    "publisher": "izaxon",
    "repository": {
        "type": "github",
        "url": "https://github.com/izaxon/vscode-todo"
    },
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "todo",
                "aliases": [
                    "todo",
                    "todo"
                ],
                "extensions": [
                    ".todo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "todo",
                "scopeName": "text.todo",
                "path": "./syntaxes/todo.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "todo",
                "path": "./snippets/todo.json"
            }
        ]
    }
}